<div id="comision">
	<h3><%= commission.name %></h3>

	<p><%= commission.explanation %></p>

	<table>
		<tr><td><strong>Miembrxs:</strong></td></tr>
		<% for(var i in commission.Users) { %>
			<tr>	
				<td></td>
				<td><%= commission.Users[i].username %></td>
			</tr>
		<% } %>	
	</table>

	<% if(session.user.isAdmin) { %>
		<div class="botones">
			<a href="/comisiones/<%= commission.id %>/add"><button id="annadir">AÃ±adir miembrx</button></a>
			<a href="/comisiones/<%= commission.id %>?_method=DELETE"><button id="borrar" onClick="return confirm('Borrar cuenta: <%= commission.name %>');">Borrar</button></a>
		</div>
	<% } %>
</div>